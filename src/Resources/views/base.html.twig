<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block head_title %}{{ admin_head_title }}{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/plugins/select2/select2.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/bootstrap/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/dist/css/skins/skin-black.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/plugins/iCheck/flat/blue.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/plugins/datepicker/datepicker3.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/plugins/daterangepicker/daterangepicker.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% block stylesheets %}{% endblock %}
</head>
<body class="hold-transition skin-black sidebar-mini">
<div class="wrapper">

    <header class="main-header">
        <!-- Logo -->
        <a href="{{ url('d01_admin_homepage') }}" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini">
                <b>{{ admin_sidebar_title_small|raw }}</b>
            </span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg">
                {{ admin_sidebar_title_big|raw }}
            </span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>

            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    {{ widgets_render('HEADER_WIDGET') }}
                    {% set sidebar_right = widgets_render('SIDEBAR_WIDGET_RIGHT') %}
                    {% if sidebar_right %}
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    {% endif %}
                </ul>
            </div>
        </nav>
    </header>

    <aside class="main-sidebar">
        <section class="sidebar">
            {{ widgets_render('SIDEBAR_WIDGET_LEFT') }}
        </section>
    </aside>

    <div class="content-wrapper">
        {% block content %}{% endblock %}
    </div>

    <footer class="main-footer">
        {% include admin_footer_template %}
    </footer>


    {% if sidebar_right %}
        <aside class="control-sidebar control-sidebar-dark">
            <div class="tab-content">
                <div class="tab-pane active">
                    {{ sidebar_right }}
                </div>
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    {% endif %}


</div>

<script src="{{ asset('bundles/adminlte/plugins/jQuery/jquery-2.2.3.min.js') }}"></script>
<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
    $.widget.bridge('uibutton', $.ui.button);
</script>
<script src="{{ asset('bundles/adminlte/bootstrap/js/bootstrap.min.js') }}"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/daterangepicker/daterangepicker.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/select2/select2.full.min.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/datepicker/bootstrap-datepicker.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
<script src="{{ asset('bundles/adminlte/plugins/fastclick/fastclick.min.js') }}"></script>
<script src="{{ asset('bundles/adminlte/dist/js/app.min.js') }}"></script>

<script>$(function(){$('select.select2').select2()});</script>
{% block javascripts %}{% endblock %}
</body>
</html>
