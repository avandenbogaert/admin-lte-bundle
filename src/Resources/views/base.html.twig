<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block meta_title %}AdminLTE 2 | Blank Page{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/adminlte/css/admin-lte.min.css') }}">
    <script src="{{ asset('bundles/adminlte/js/options.js') }}"></script>
    {% block head %}{% endblock %}
    {% block stylesheets %}{% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body {% block body_attributes %}{% endblock %}>

{% block body %}{% endblock %}

<script src="{{ asset('bundles/adminlte/js/admin-lte.min.js') }}"></script>
<script>
    $(function () {
        $('input').iCheck(AdminLTE.options.iCheck);
        $('select').select2(AdminLTE.options.select2);
        $('input.datepicker').datepicker(AdminLTE.options.datepicker);
    });
</script>
{% block javascripts %}
{% endblock %}
</body>
</html>
